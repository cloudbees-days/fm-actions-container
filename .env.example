# CloudBees Platform API Configuration
# Copy this file to .env and fill in your actual values

# Required: Your CloudBees Platform Personal Access Token
# Get this from your CloudBees Platform user profile -> Security -> API Tokens
CLOUDBEES_TOKEN=your-personal-access-token-here

# Required: Your organization id
CLOUDBEES_ORG_ID=12345678-1234-1234-1234-123456789012

# Optional: CloudBees Platform API URL (defaults to https://api.cloudbees.io)
CLOUDBEES_API_URL=https://api.cloudbees.io

# Test flag name for testing
TEST_FLAG_NAME=test-feature-flag

# Test environment name for testing
TEST_ENVIRONMENT_NAME=development

TEST_APPLICATION_NAME=my-app

# Optional: Enable verbose output for testing
VERBOSE=true 